[icinga-snapshot-builds]
name=ICINGA (snapshot builds for epel)
{% if repo_icinga__mirror_url is defined and repo_icinga__mirror_url|length %}
baseurl={{ repo_icinga__mirror_url }}/{{ repo_icinga__snapshot_url_suffix }}
{% else %}
baseurl=https://packages.icinga.com/{{ repo_icinga__snapshot_url_suffix }}
{% endif %}
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ICINGA
metadata_expire=1d
