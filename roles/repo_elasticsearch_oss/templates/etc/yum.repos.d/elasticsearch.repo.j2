# {{ ansible_managed }}
# 2023122901

[elasticsearch-{{ repo_elasticsearch_oss__version__combined_var }}]
name=Elasticsearch repository for {{ repo_elasticsearch_oss__version__combined_var }} packages
{% if repo_elasticsearch_oss__mirror_url is defined and repo_elasticsearch_oss__mirror_url | length %}
baseurl={{ repo_elasticsearch_oss__mirror_url }}/elasticsearch-oss/packages/{{ repo_elasticsearch_oss__version__combined_var }}/yum
{% else %}
baseurl=https://artifacts.elastic.co/packages/oss-{{ repo_elasticsearch_oss__version__combined_var }}/yum
{% endif %}
gpgcheck=1
gpgkey=https://artifacts.elastic.co/GPG-KEY-elasticsearch
enabled=1
autorefresh=1
type=rpm-md
{% if repo_elasticsearch_oss__basic_auth_login is defined and repo_elasticsearch_oss__basic_auth_login | length %}
username={{ repo_elasticsearch_oss__basic_auth_login["username"] }}
password={{ repo_elasticsearch_oss__basic_auth_login["password"] }}
{% endif %}
