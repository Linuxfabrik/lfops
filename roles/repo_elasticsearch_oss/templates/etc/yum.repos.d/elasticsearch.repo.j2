# {{ ansible_managed }}
# 2022100401

[elasticsearch-{{ repo_elasticsearch_oss__version }}]
name=Elasticsearch repository for {{ repo_elasticsearch_oss__version }} packages
{% if repo_elasticsearch_oss__mirror_url is defined and repo_elasticsearch_oss__mirror_url | length %}
baseurl={{ repo_elasticsearch_oss__mirror_url }}/oss-{{ repo_elasticsearch_oss__version }}/yum
{% else %}
baseurl=https://artifacts.elastic.co/packages/oss-{{ repo_elasticsearch_oss__version }}/yum
{% endif %}
gpgcheck=1
gpgkey=https://artifacts.elastic.co/GPG-KEY-elasticsearch
enabled=1
autorefresh=1
type=rpm-md
