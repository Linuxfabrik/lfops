# {{ ansible_managed }}
# 2020091001

# This repository only provides GLPI 9.2 and its plugins
# NOTICE: you need at least PHP 5.6

[remi-glpi92]
name=Remi's GLPI 9.2 RPM repository for Enterprise Linux 7 - $basearch
{% if repo_remi__mirror_url is defined and repo_remi__mirror_url | length %}
baseurl={{ repo_remi__mirror_url }}/remi/enterprise/7/glpi92/x86_64/
{% else %}
#baseurl=http://rpms.remirepo.net/enterprise/7/glpi92/$basearch/
#mirrorlist=https://rpms.remirepo.net/enterprise/7/glpi92/httpsmirror
mirrorlist=http://cdn.remirepo.net/enterprise/7/glpi92/mirror
{% endif %}
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-remi
