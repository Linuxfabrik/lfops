[remi-debuginfo]
name=Remi's RPM repository for Fedora $releasever - $basearch - debuginfo
{% if repo_remi__mirror_url is defined and repo_remi__mirror_url | length %}
baseurl={{ repo_remi__mirror_url }}/fedora/$releasever/debug-remi/$basearch/
{% else %}
baseurl=http://rpms.remirepo.net/fedora/$releasever/debug-remi/$basearch/
{% endif %}
enabled=0
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-remi-$releasever
{% if repo_remi__basic_auth_login is defined and repo_remi__basic_auth_login | length %}
username={{ repo_remi__basic_auth_login["username"] }}
password={{ repo_remi__basic_auth_login["password"] }}
{% endif %}

