# {{ ansible_managed }}
# 2022102101

# https://docs.graylog.org/docs/operating-system-packages
[graylog-{{ repo_graylog__version }}]
name=graylog-{{ repo_graylog__version }}
{% if repo_graylog__mirror_url is defined and repo_graylog__mirror_url | length %}
baseurl={{ repo_graylog__mirror_url }}/repo/el/stable/{{ repo_graylog__version }}/$basearch/
{% else %}
baseurl=https://packages.graylog2.org/repo/el/stable/{{ repo_graylog__version }}/$basearch/
{% endif %}
gpgcheck=1
repo_gpgcheck=0
gpgkey=https://raw.githubusercontent.com/Graylog2/fpm-recipes/{{ repo_graylog__version }}/recipes/graylog-repository/files/rpm/RPM-GPG-KEY-graylog
